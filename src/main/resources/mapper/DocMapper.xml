<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyn.wiki.mapper.DocMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lyn.wiki.domain.Doc">
        <id column="id" property="id" />
        <result column="ebook_id" property="ebook_id" />
        <result column="parent" property="parent" />
        <result column="name" property="name" />
        <result column="sort" property="sort" />
        <result column="view_count" property="view_count" />
        <result column="vote_count" property="vote_count" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ebook_id, parent, name, sort, view_count, vote_count
    </sql>

</mapper>
